              <div class="col-xs-7">
                <h4><%= Faker::StarWars.droid %></h4>
              </div>
              <div class="col-xs-5 text-right">
                <h4 id="ERROR_status"><%= Faker::Number.between(1, 100) %></h4>
              </div>
            </div>
          </div>
          <%= link_to('#') do %>
            <div class="panel-footer">
              <span class="pull-left">View Details</span>
